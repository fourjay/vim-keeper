{
    "tests/*.vader" : {
        "type"      : "test",
        "alternate" : "autoload/keeper/{}.vim"
    },
    "README.md" : {
        "type"      : "readme",
        "projectbase" : "./"
    },
    "plugin/*" : {
        "type"      : "plugin"
    },
    "autoload/keeper/*.vim" : {
        "type"      : "autoload",
        "alternate" : "tests/{}.vader"
    }
}
