{
    "tests/*.vader" : {
        "type"      : "test",
        "alternate" : "autoload/keeper/{}.vim"
    },
    "autoload/keeper/*.vim" : {
        "type"      : "autoload",
        "alternate" : "tests/{}.vader"
    },
    "README.md"  : {"type"      : "readme"},
    "plugin/*"   : {"type"      : "plugin"},
    "ftplugin/*" : {"type"      : "ftplugin"},
    "syntax/*"   : {"type"      : "syntax"},
    "doc/*.txt"  : {"type"      : "doc"}
}
